Block 'VertexInput'
{
  Type = VertexInput
  
  Node 'Position'
  {
    Type = Out
    Format = Vector
    Editable = false
    IOSemantic = VertexPosition
  }
  Node 'Color'
  {
    Type = Out
    Format = Vector
    Editable = false
    IOSemantic = Color0
